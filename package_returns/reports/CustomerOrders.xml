<!DOCTYPE openRPTDef>
<report>
 <title>Customer Order Report</title>
 <name>CustomerOrders</name>
 <description>Customer Order History report. Has 2014-2015 year default.	

</description>
 <parameter default="" active="true" listtype="dynamic" type="string" name="customer">
  <description>Choose from Active customer accounts that are not of type Internal, Inactive Member, or Sponsor</description>
  <query>SELECT DISTINCT
  cust_name,   cust_name
FROM custinfo
join custtype on cust_custtype_id = custtype_id
WHERE 
cust_active
and NOT custtype_code IN ('INTERNAL','INACTIVE MEMBER','SPONSOR')
ORDER BY 1</query>
 </parameter>
 <parameter default="6/30/15" active="true" listtype="static" type="string" name="enddate">
  <description>ENTER End Date for the Report as M/D/YY</description>
 </parameter>
 <parameter default="7/1/14" active="true" listtype="static" type="string" name="startdate">
  <description>ENTER Report Start Date as M/D/YY</description>
 </parameter>
 <grid>
  <snap/>
  <show/>
  <x>0.05</x>
  <y>0.05</y>
 </grid>
 <size>Letter</size>
 <portrait/>
 <topmargin>50</topmargin>
 <bottommargin>50</bottommargin>
 <rightmargin>28</rightmargin>
 <leftmargin>28</leftmargin>
 <querysource loadFromDb="true">
  <name>logo</name>
  <mqlgroup>AAA_ASSET</mqlgroup>
  <mqlname>logo</mqlname>
 </querysource>
 <querysource>
  <name>order_history</name>
  <sql>select delivery_date,
return_date,
order_shiptoname,
item_description,
qty_ordered,
order_num,
cust_name
from _return.order_history
where cust_name = &lt;? value("customer") ?>
and delivery_date between &lt;? value("startdate") ?> and &lt;? value("enddate") ?>
order by delivery_date,order_num,order_shiptoname</sql>
 </querysource>
